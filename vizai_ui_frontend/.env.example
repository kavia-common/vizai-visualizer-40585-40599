# Public env vars consumed by the UI (Create React App format)
# Note: Keep data mocked for Phase-1. These are future-ready and used for labels and base URLs.
REACT_APP_API_BASE=https://api.example.com
REACT_APP_BACKEND_URL=https://backend.example.com
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=wss://ws.example.com
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=1
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=0
REACT_APP_LOG_LEVEL=info
# Feature flags as JSON string. Example: {"chat":true}
REACT_APP_FEATURE_FLAGS={"chat":false}
REACT_APP_EXPERIMENTS_ENABLED=0
